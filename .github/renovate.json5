{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>statnett/renovate-presets:default.json5",
    ":semanticCommitTypeAll(ci)",
  ],
  "labels": ["dependencies"],
  "ignorePaths": [],
  "golang": {
    "enabled": false,
  },
  "github-actions": {
    "enabled": false,
  },
  "packageRules": [
    {
      "matchPackageNames": [
        "ghcr.io/aquasecurity/trivy",
      ],
      "semanticCommitType": "build",
    },
  ],
}
