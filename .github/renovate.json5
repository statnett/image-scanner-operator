{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["github>statnett/renovate-config"],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "digest"],
      "automerge": true,
    },
    {
      "matchPackageNames": [
        "ghcr.io/aquasecurity/trivy",
      ],
      "semanticCommitType": "fix",
    },
  ],
}
